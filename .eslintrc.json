{
  "parserOptions":{
    "ecmaFeatures":{
      "jsx":true
    }
  },
  "env":{
    "browser": true,
    "node":true,
    "jquery":true
  },
  "rules":{
    "quotes":1,
    "no-trailing-spaces":1,
    "eol-last":1,
    "no-unused-vars":0,
    "no-underscore-dongles":0,
    "no-alert":1,
    "no-lone-blocks":1
  },
  "globals":{
    "jQuery":true,
    "$":true
  }
}